{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": [
            1,
            4
        ],
        "quotes": 0,
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "guard-for-in": 2,
        "no-extend-native": 2,
        "no-loop-func": 0,
        "no-unused-vars": 1,
        "strict": 0,
        "no-unused-expressions": 0,
        "comma-spacing": 0,
        "eqeqeq": [
            2,
            "allow-null"
        ]
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "globals": {
        "$": true,
        "idbCaptain": true
    }
}